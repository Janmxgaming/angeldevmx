{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@config/*": ["src/config/*"],
      "@constants/*": ["src/constants/*"],
      "@services/*": ["src/services/*"],
      "@pages/*": ["src/pages/*"],
      "@context/*": ["src/context/*"]
    }
  },
  "include": ["src"]
}
